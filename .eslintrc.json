
{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true, 
          "semi": false,
          "trailingComma": "es5",
          "printWidth": 80,
          "tabWidth": 2
        }
      ],
      "react/prop-types": "off"
    }
  }
  